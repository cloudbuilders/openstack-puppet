[DEFAULT]
bind_ip = <%= if has_variable?('swift_storage_net_interface') then eval("ipaddress_#{swift_storage_net_interface}") else host_mgmt_ip end %>
workers = 2

[pipeline:main]
pipeline = container-server

[app:container-server]
use = egg:swift#container

[container-replicator]

[container-updater]

[container-auditor]
