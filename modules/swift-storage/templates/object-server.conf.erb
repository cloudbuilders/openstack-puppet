[DEFAULT]
bind_ip = <%= if has_variable?('swift_storage_net_interface') then eval("ipaddress_#{swift_storage_net_interface}") else mgmt_ip end %>
workers = 2

[pipeline:main]
pipeline = object-server

[app:object-server]
use = egg:swift#object

[object-replicator]

[object-updater]

[object-auditor]
