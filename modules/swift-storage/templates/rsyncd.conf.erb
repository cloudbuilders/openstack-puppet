uid = swift
gid = swift

# this should really be bound on an internal-only network
log file = /var/log/rsyncd.log
pid file = /var/run/rsyncd.pid
address = <%= eval("ipaddress_#{swift_storage_net_interface}") %>

[account]
max connections = 2
path = /srv/node/
read only = false
lock file = /var/lock/account.lock

[container]
max connections = 2
path = /srv/node/
read only = false
lock file = /var/lock/container.lock

[object]
max connections = 2
path = /srv/node/
read only = false
lock file = /var/lock/object.lock
